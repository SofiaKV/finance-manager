{
  "name": "finance-manager",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "packageManager": "pnpm@10.18.0",
  "scripts": {
    "prepare": "husky",
    "install:all": "pnpm install --filter . --filter ./backend --filter ./frontend",
    "dev": "pnpm -r build && pnpm start:dev",
    "start:dev": "concurrently -n BACKEND,FRONTEND -c blue,magenta \"pnpm --filter=backend start:dev\" \"pnpm --filter=frontend dev\"",
    "format:check": "pnpm -r format:check",
    "format:fix": "pnpm -r format:fix",
    "lint:check": "pnpm -r lint:check",
    "lint:fix": "pnpm -r lint:fix",
    "test": "pnpm -r run test",
    "build": "pnpm -r run build"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/js": "^9.0.0",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "globals": "^16.4.0",
    "husky": "^9.1.6",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.46.0"
  },
  "optionalDependencies": {
    "concurrently": "^9.1.0"
  },
  "dependencies": {
    "@types/node": "^22.18.8",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2"
  }
}
